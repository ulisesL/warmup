
/* This file contains the style parameters for the sample client. */


.box {
    width: 300px;
    height: 120px;
    border: thin orange solid;
    background-color: rgb(223,243,263);
}

.errbox {
    width: 300px;
    height: 120px;
    border: thin maroon solid;
    background-color: rgb(243,210,213);
}

.message {
    margin: 10px 20px 10px 20px;
    border: thin black dotted;
}

.errmessage{
    margin: 10px 20px 10px 20px;
    border: thin white dotted;
    color: red;
}

body {
    font-family: 'Helvetica Neue Light', HelveticaNeue-Light, 'Helvetica Neue', Helvetica, Arial, sans-serif;
}
